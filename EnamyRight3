package fish;

import java.awt.Rectangle;

public class EnamyRight3 extends Enamy{
	EnamyRight3(){
		this.x=1400;
		dir=-1;
		this.y=(int)(Math.random()*700+100);
		this.width=300;
		this.height=150;
		this.speed=10;
		this.count=3;
		this.type=3;
		this.img=GameUtils.enamyr3;
	}
	public Rectangle getRec() {
		return new Rectangle(x+40,y+30,width-80,height-60);
	}
}
